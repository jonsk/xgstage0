# configuration files for xiange stage0 build scripts

export CC="gcc"
export CFLAGS=""

#cross compile building platform (autodetect)
CLFS_BUILD="$MACHTYPE"

#cross compile host/target platform (mips,little endian)
CLFS_TARGET="mipsel-unknown-linux-gnu"

#ABI
#export ABI="32"
export MABI="n32"

CFLAGS_FOR_TARGET="-mabi=n32 -march=loongson2f -pipe -Os "
CXXFLAGS_FOR_TARGET="-mabi=n32 -march=loongson2f -pipe -Os "
LIBCFLAGS_FOR_TARGET="-mabi=n32 -march=loongson2f -pipe -Os "
